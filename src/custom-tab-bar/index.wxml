<van-tabbar active-color="rgb(226,35,25)" active="{{ active }}" bind:change="onChange">
    <van-tabbar-item
            class="{{item.disable ? 'disable' : ''}}"
            wx:for="{{ list }}"
            wx:key="index"
            icon="{{ item.icon }}">
        {{item.text}}
    </van-tabbar-item>
</van-tabbar>